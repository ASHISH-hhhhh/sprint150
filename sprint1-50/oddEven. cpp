#include<iostream>
using namespace std;

int main()
{
    cout << "**********************Sprint150!*****************************" << endl;
    int oddEven;
    cout<<"Enter the Number :";
    cin>>oddEven;
    if(oddEven%2==0) {
        cout<<"Even"<<endl ;
    } else {
        cout<<"Odd" <<endl;
    }
    return 0;
}